{
    "SUBSCRIBERS": {
        "summary": "Get the pubsub subscribers, filter by type/pattern.",
        "complexity": "O(N * M) where N is the number of matched channel/patterns and M is the number of subscribed clients",
        "group": "pubsub",
        "since": "7.2.0",
        "arity": -2,
        "container": "PUBSUB",
        "function": "pubsubCommand",
        "command_flags": [
            "PUBSUB",
            "LOADING",
            "STALE"
        ],
        "arguments": [
            {
                "name": "subscription-type",
                "type": "oneof",
                "arguments": [
                    {
                        "name": "channel",
                        "type": "pure-token",
                        "token": "CHANNEL"
                    },
                    {
                        "name": "shard-channel",
                        "type": "pure-token",
                        "token": "SHARD-CHANNEL"
                    },
                    {
                        "name": "pattern",
                        "type": "pure-token",
                        "token": "PATTERN"
                    }
                ]
            },
            {
                "token": "MATCH",
                "name": "pattern",
                "type": "string",
                "optional": true
            },
            {
                "token": "COUNT",
                "name": "count",
                "type": "integer",
                "optional": true
            }
        ],
        "reply_schema": {
            "description": "A map where each key is a channel name/pattern, and each value is the list of subscriptions",
            "type": "object",
            "patternProperties": {
                "^.*$": {
                    "type": "array",
                    "minItems": 0,
                    "maxItems": 4294967295,
                    "items": [
                        {
                            "description": "information about the subscribed client",
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "id": {
                                    "type": "string"
                                },
                                "addr": {
                                    "type": "string"
                                }
                            }
                        }
                    ]
                }
            }
        }
    }
}
