{
    "DEBUG": {
        "summary": "Lists or updates the current configurable parameters of Redis Sentinel.",
        "complexity": "O(N) where N is the number of configurable parameters",
        "group": "sentinel",
        "since": "7.0.0",
        "arity": -2,
        "container": "SENTINEL",
        "function": "sentinelCommand",
        "command_flags": [
            "ADMIN",
            "SENTINEL",
            "ONLY_SENTINEL"
        ],
        "reply_schema": {
            "oneOf": [
                {
                    "description": "The configuration update was successful.",
                    "const": "OK"
                },
                {
                    "description": "List of configurable time parameters and their values (milliseconds).",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "INFO-PERIOD": {
                            "type": "string",
                            "description": "sentinel_info_period."
                        },
                        "PING-PERIOD": {
                            "type": "string",
                            "description": "sentinel_ping_period."
                        },
                        "ASK-PERIOD": {
                            "type": "string",
                            "description": "sentinel_ask_period."
                        },
                        "PUBLISH-PERIOD": {
                            "type": "string",
                            "description": "sentinel_publish_period."
                        },
                        "DEFAULT-DOWN-AFTER": {
                            "type": "string",
                            "description": "sentinel_default_down_after."
                        },
                        "DEFAULT-FAILOVER-TIMEOUT": {
                            "type": "string",
                            "description": "sentinel_default_failover_timeout."
                        },
                        "TILT-TRIGGER": {
                            "type": "string",
                            "description": "sentinel_tilt_trigger."
                        },
                        "TILT-PERIOD": {
                            "type": "string",
                            "description": "sentinel_tilt_period."
                        },
                        "SLAVE-RECONF-TIMEOUT": {
                            "type": "string",
                            "description": "sentinel_slave_reconf_timeout."
                        },
                        "MIN-LINK-RECONNECT-PERIOD": {
                            "type": "string",
                            "description": "sentinel_min_link_reconnect_period."
                        },
                        "ELECTION-TIMEOUT": {
                            "type": "string",
                            "description": "sentinel_election_timeout."
                        },
                        "SCRIPT-MAX-RUNTIME": {
                            "type": "string",
                            "description": "sentinel_script_max_runtime."
                        },
                        "SCRIPT-RETRY-DELAY": {
                            "type": "string",
                            "description": "sentinel_script_retry_delay."
                        }
                    }
                }
            ]
        },
        "arguments": [
            {
                "name": "data",
                "type": "block",
                "optional": true,
                "multiple": true,
                "arguments": [
                    {
                        "name": "parameter",
                        "type": "string"
                    },
                    {
                        "name": "value",
                        "type": "string"
                    }
                ]
            }
        ]
    }
}
