{
    "ZUNIONSTORE":{
        "summary":"Add multiple sorted sets and store the resulting sorted set in a new key",
        "complexity":"O(N)+O(M log(M)) with N being the sum of the sizes of the input sorted sets, and M being the number of elements in the resulting sorted set.",
        "since":"2.0.0",
        "group":"sorted_set",
        "return_summary":"@integer-reply: the number of elements in the resulting sorted set at\n`destination`.",
        "return_types":{
            "2":[
                [
                    "integer",
                    "Cardinality of the result"
                ]
            ],
            "3":[
                [
                    "integer",
                    "Cardinality of the result"
                ]
            ]
        },
        "arguments":[
            {
                "name":"destination",
                "type":"key",
                "value":"destination"
            },
            {
                "name":"numkeys",
                "type":"integer",
                "value":"numkeys"
            },
            {
                "name":"key",
                "type":"key",
                "value":"key",
                "multiple":true
            },
            {
                "name":"weights",
                "type":"integer",
                "token":"WEIGHTS",
                "value":"weight",
                "multiple":true,
                "optional":true
            },
            {
                "name":"aggregate",
                "type":"oneof",
                "token":"AGGREGATE",
                "value":[
                    {
                        "name":"sum",
                        "token":"SUM"
                    },
                    {
                        "name":"min",
                        "token":"MIN"
                    },
                    {
                        "name":"max",
                        "token":"MAX"
                    }
                ],
                "optional":true
            }
        ],
        "arity":-4,
        "command_flags":[
            "write",
            "use-memory"
        ],
        "acl_categories":[
            "sortedset",
            "write",
            "slow"
        ],
        "key_specs":[
            {
                "begin_search":{
                    "index":{
                        "pos":1
                    }
                },
                "find_keys":{
                    "range":{
                        "lastkey":0,
                        "step":1,
                        "limit":0
                    }
                },
                "flags":[
                    "write"
                ]
            },
            {
                "begin_search":{
                    "index":{
                        "pos":1
                    }
                },
                "find_keys":{
                    "keynum":{
                        "keynumidx":0,
                        "firstkey":1,
                        "step":1
                    }
                },
                "flags":[
                    "read"
                ]
            }
        ],
        "function": "zunionstoreCommand",
        "get_keys_function": "zunionInterDiffStoreGetKeys"
    }
}
